---
layout: 'singlepage.njk'
title: Brand & stuff
class: comms
permalink: "/comms.html"
---

<section class="gallery">
  {# // Loop through, and create markup, for all images in "/images/comms" #}
  {% for image in collections.commsImages%}

    {% if not '.mp4' in image %}

      <figure>
        <img src="{{image | replace('static', '')}}" alt="">
        <p>
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M3 15v4c0 1.1.9 2 2 2h14a2 2 0 0 0 2-2v-4M17 9l-5 5-5-5M12 12.8V2.5"/></svg>
          <a download href={{image | replace('static', '') | safe}}>
            {{image | replace('static/images/comms/', '') | safe}}</a>
        </p>
      </figure>

    {% elseif '.mp4' in image %}
      <figure>
        <video controls>
          <source src="{{image | replace('static', '') | safe}}" type="video/mp4">
        </video>
        <p>
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M3 15v4c0 1.1.9 2 2 2h14a2 2 0 0 0 2-2v-4M17 9l-5 5-5-5M12 12.8V2.5"/></svg>

          <a download href={{image | replace('static', '') | safe}}>
            {{image | replace('static/images/comms/', '') | safe}}</a>
        </p>
      </figure>
    {% endif %}
  {% endfor %}
</section>